{"ast":null,"code":"import { ref } from \"vue\"; // Vue 3, or use Vue.observable for Vue 2\nimport SideBar from \"@/components/layouts/SideBar.vue\";\nimport TopNavigator from \"@/components/UI/TopNavigator.vue\";\nimport BaseTitle from \"@/components/UI/BaseTitle.vue\";\nimport BaseSubtitle from \"@/components/UI/BaseSubtitle.vue\";\nexport default {\n  components: {\n    SideBar,\n    TopNavigator,\n    BaseTitle,\n    BaseSubtitle\n  },\n  props: [\"page-title\", \"top-title\", \"top-subtitle\", \"bg-image\"],\n  provide() {\n    const isSidebar = ref(true); // Reactive isSidebar\n    return {\n      setGridLayout: () => isSidebar.value = !isSidebar.value,\n      isSidebar\n    };\n  },\n  computed: {\n    compGridLayout() {\n      return this.isSidebar.value ? \"ten-nine\" : \"five-ninety-five\"; // Adjust for Vue 2 if necessary\n    }\n  }\n};","map":{"version":3,"names":["ref","SideBar","TopNavigator","BaseTitle","BaseSubtitle","components","props","provide","isSidebar","setGridLayout","value","computed","compGridLayout"],"sources":["C:\\Users\\Nihil\\Desktop\\re-licts\\src\\layouts\\MainLayout.vue"],"sourcesContent":["<template>\r\n  <section\r\n    id=\"container-main\"\r\n    :style=\"{ backgroundImage: `url(${require(`@/assets/pics/${bgImage}`)})` }\"\r\n    :class=\"compGridLayout\"\r\n  >\r\n    <side-bar></side-bar>\r\n    <section id=\"side-right\">\r\n      <top-navigator :page-title=\"pageTitle\"></top-navigator>\r\n      <div id=\"container-top\">\r\n        <base-title :title=\"topTitle\"></base-title>\r\n        <base-subtitle :title=\"topSubtitle\"></base-subtitle>\r\n      </div>\r\n      <slot name=\"mainContent\"></slot>\r\n    </section>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from \"vue\"; // Vue 3, or use Vue.observable for Vue 2\r\nimport SideBar from \"@/components/layouts/SideBar.vue\";\r\nimport TopNavigator from \"@/components/UI/TopNavigator.vue\";\r\nimport BaseTitle from \"@/components/UI/BaseTitle.vue\";\r\