{"ast":null,"code":"import { ref, provide } from \"vue\";\nimport SideBar from \"@/components/layouts/SideBar.vue\";\nimport TopNavigator from \"@/components/UI/TopNavigator.vue\";\nimport BaseTitle from \"@/components/UI/BaseTitle.vue\";\nimport BaseSubtitle from \"@/components/UI/BaseSubtitle.vue\";\nexport default {\n  components: {\n    SideBar,\n    TopNavigator,\n    BaseTitle,\n    BaseSubtitle\n  },\n  setup() {\n    // Reactive sidebar state\n    const isSidebar = ref(true);\n\n    // Toggle sidebar visibility\n    const toggleSidebar = () => {\n      isSidebar.value = !isSidebar.value;\n    };\n\n    // Provide reactive properties\n    provide(\"isSidebar\", isSidebar);\n    provide(\"setGridLayout\", toggleSidebar);\n\n    // Props and computed values\n    const bgImage = \"background.jpg\"; // Example background image\n    const pageTitle = \"Dashboard\";\n    const topTitle = \"Welcome to the Dashboard\";\n    const topSubtitle = \"Manage your content efficiently\";\n    const compGridLayout = computed(() => isSidebar.value ? \"ten-nine\" : \"five-ninety-five\");\n    return {\n      bgImage,\n      pageTitle,\n      topTitle,\n      topSubtitle,\n      toggleSidebar,\n      compGridLayout\n    };\n  }\n};","map":{"version":3,"names":["ref","provide","SideBar","TopNavigator","BaseTitle","BaseSubtitle","components","setup","isSidebar","toggleSidebar","value","bgImage","pageTitle","topTitle","topSubtitle","compGridLayout","computed"],"sources":["C:\\Users\\Nihil\\Desktop\\re-licts\\src\\layouts\\MainLayout.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <!-- Toggle Button to Show/Hide Sidebar -->\r\n    <button @click=\"toggleSidebar\">Toggle Sidebar</button>\r\n\r\n    <!-- Main Layout -->\r\n    <section\r\n      id=\"container-main\"\r\n      :style=\"{\r\n        backgroundImage: `url(${require(`@/assets/pics/${bgImage}`)})`,\r\n      }\"\r\n      :class=\"compGridLayout\"\r\n    >\r\n      <side-bar></side-bar>\r\n      <section id=\"side-right\">\r\n        <top-navigator :page-title=\"pageTitle\"></top-navigator>\r\n        <div id=\"container-top\">\r\n          <base-title :title=\"topTitle\"></base-title>\r\n          <base-subtitle :title=\"topSubtitle\"></base-subtitle>\r\n        </div>\r\n        <slot name=\"mainContent\"></slot>\r\n      </section>\r\n    </section>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { ref, provide } from \"vue\";\r\nimport SideBar from \"@/components/layouts/SideBar.vue\";\r\nimport TopNavigator from \"@/components/UI/TopNavigator.vue\";\r\nimport BaseTitle from \"@/components/UI/BaseTitle.vue\";\r\nimport BaseSubtitle from \"@/components/UI/BaseSubtitle.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    SideBar,\r\n    TopNavigator,\r\n    BaseTitle,\r\n    BaseSubtitle,\r\n  },\r\n  setup() {\r\n    // Reactive sidebar state\r\n    const isSidebar = ref(true);\r\n\r\n    // Toggle sidebar visibility\r\n    const toggleSidebar = () => {\r\n      isSidebar.value = !isSidebar.value;\r\n    };\r\n\r\n    // Provide reactive properties\r\n    provide(\"isSidebar\", isSidebar);\r\n    provide(\"setGridLayout\", toggleSidebar);\r\n\r\n    // Props and computed values\r\n    const bgImage = \"background.jpg\"; // Example background image\r\n    const pageTitle = \"Dashboard\";\r\n    const topTitle = \"Welcome to the Dashboard\";\r\n    const topSubtitle = \"Manage your content efficiently\";\r\n\r\n    const compGridLayout = computed(() =>\r\n      isSidebar.value ? \"ten-nine\" : \"five-ninety-five\"\r\n    );\r\n\r\n    return {\r\n      bgImage,\r\n      pageTitle,\r\n      topTitle,\r\n      topSubtitle,\r\n      toggleSidebar,\r\n      compGridLayout,\r\n    };\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.ten-nine {\r\n  display: grid;\r\n  grid-template-columns: 10fr 9fr;\r\n}\r\n\r\n.five-ninety-five {\r\n  display: grid;\r\n  grid-template-columns: 5fr 95fr;\r\n}\r\n</style>\r\n"],"mappings":"AA2BA,SAASA,GAAG,EAAEC,OAAM,QAAS,KAAK;AAClC,OAAOC,OAAM,MA