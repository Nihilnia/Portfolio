{"ast":null,"code":"import { createRouter, createWebHistory } from \"vue-router\";\nimport MainLayout from \"./layouts/MainLayout.vue\";\nimport HomePage from \"@/pages/HomePage.vue\"; // Example component\n\nexport default {\n  provide() {\n    return {\n      setCurrentPage: this.setCurrentPage\n    };\n  },\n  components: {\n    MainLayout\n  },\n  data() {\n    return {\n      currentPage: \"Home\",\n      // Set default value\n      topTitle: \"Hello world!\",\n      // Set default title\n      topSubtitle: \"Welcome to my website\",\n      // Set default subtitle\n      bgImage: \"a.jpg\" // Set default background image\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${require(`@/assets/pics/${this.bgImage}`)})`,\n        backgroundBlendMode: \"overlay\",\n        backgroundColor: \"rgba(0, 0, 0, 0.65)\",\n        backgroundRepeat: \"no-repeat\",\n        backgroundSize: \"cover\"\n      };\n    }\n  },\n  methods: {\n    setCurrentPage(page, topT, subTT, bgImage) {\n      this.currentPage = page;\n      this.topTitle = topT;\n      this.topSubtitle = subTT;\n      this.bgImage = bgImage || \"a.jpg\"; // Default to \"a.jpg\" if no image provided\n    }\n  },\n  mounted() {\n    // Call setCurrentPage to set initial values on load\n    this.setCurrentPage(\"Home\", \"Main Title\", \"Welcome to my page\", \"background.jpg\");\n  },\n  router: createRouter({\n    history: createWebHistory(process.env.BASE_URL),\n    routes: [{\n      path: \"/\",\n      name: \"home\",\n      component: HomePage\n    }\n    // Add other routes here as needed\n    ]\n  })\n};","map":{"version":3,"names":["createRouter","createWebHistory","MainLayout","HomePage","provide","setCurrentPage","components","data","currentPage","topTitle","topSubtitle","bgImage","computed","backgroundStyle","backgroundImage","require","backgroundBlendMode","backgroundColor","backgroundRepeat","backgroundSize","methods","page","topT","subTT","mounted","router","history","process","env","BASE_URL","routes","path","name","component"],"sources":["C:\\Users\\Nihil\\Desktop\\aaa\\Portfolio\\src\\App.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-wrapper\" :style=\"backgroundStyle\">\r\n    <main-layout\r\n      :page-title=\"currentPage\"\r\n      :top-title=\"topTitle\"\r\n      :top-subtitle=\"topSubtitle\"\r\n    >\r\n      <template #mainContent>\r\n        <router-view />\r\n      </template>\r\n    </main-layout>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { createRouter, createWebHistory } from \"vue-router\";\r\nimport MainLayout from \"./layouts/MainLayout.vue\";\r\nimport HomePage from \"@/pages/HomePage.vue\"; // Example component\r\n\r\nexport default {\r\n  provide() {\r\n    return {\r\n      setCurrentPage: this.setCurrentPage,\r\n    };\r\n  },\r\n  components: {\r\n    MainLayout,\r\n  },\r\n  data() {\r\n    return {\r\n      currentPage: \"Home\", // Set default value\r\n      topTitle: \"Hello world!\", // Set default title\r\n      topSubtitle: \"Welcome to my website\", // Set default subtitle\r\n      bgImage: \"a.jpg\", // Set default background image\r\n    };\r\n  },\r\n  computed: {\r\n    backgroundStyle() {\r\n      return {\r\n        backgroundImage: `url(${require(`@/assets/pics/${this.bgImage}`)})`,\r\n        backgroundBlendMode: \"overlay\",\r\n        backgroundColor: \"rgba(0, 0, 0, 0.65)\",\r\n        backgroundRepeat: \"no-repeat\",\r\n        backgroundSize: \"cover\",\r\n      };\r\n    },\r\n  },\r\n  methods: {\r\n    setCurrentPage(page, topT, subTT, bgImage) {\r\n      this.currentPage = page;\r\n      this.topTitle = topT;\r\n      this.topSubtitle = subTT;\r\n      this.bgImage = bgImage || \"a.jpg\"; // Default to \"a.jpg\" if no image provided\r\n    },\r\n  },\r\n  mounted() {\r\n    // Call setCurrentPage to set initial values on load\r\n    this.setCurrentPage(\r\n      \"Home\",\r\n      \"Main Title\",\r\n      \"Welcome to my page\",\r\n      \"background.jpg\"\r\n    );\r\n  },\r\n  router: createRouter({\r\n    history: createWebHistory(process.env.BASE_URL),\r\n    routes: [\r\n      {\r\n        path: \"/\",\r\n        name: \"home\",\r\n        component: HomePage,\r\n      },\r\n      // Add other routes here as needed\r\n    ],\r\n  }),\r\n};\r\n</script>\r\n\r\n<style>\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n  font-family: \"Poiret One\", serif;\r\n}\r\n\r\n.app-wrapper {\r\n  height: 100vh;\r\n  overflow: hidden;\r\n}\r\n\r\n@media all and (max-width: 1366px) {\r\n  .app-wrapper {\r\n    height: auto;\r\n    min-height: 100vh;\r\n  }\r\n}\r\n\r\n@media all and (max-width: 1200px) {\r\n  .app-wrapper {\r\n    height: auto;\r\n    min-height: 100vh;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAeA,SAASA,YAAY,EAAEC,gBAAe,QAAS,YAAY;AAC3D,OAAOC,UAAS,MAAO,0BAA0B;AACjD,OAAOC,QAAO,MAAO,sBAAsB,EAAE;;AAE7C,eAAe;EACbC,OAAOA,CAAA,EAAG;IACR,OAAO;MACLC,cAAc,EAAE,IAAI,CAACA;IACvB,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVJ;EACF,CAAC;EACDK,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAW,EAAE,MAAM;MAAE;MACrBC,QAAQ,EAAE,cAAc;MAAE;MAC1BC,WAAW,EAAE,uBAAuB;MAAE;MACtCC,OAAO,EAAE,OAAO,CAAE;IACpB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,eAAeA,CAAA,EAAG;MAChB,OAAO;QACLC,eAAe,EAAE,OAAOC,OAAO,CAAC,iBAAiB,IAAI,CAACJ,OAAO,EAAE,CAAC,GAAG;QACnEK,mBAAmB,EAAE,SAAS;QAC9BC,eAAe,EAAE,qBAAqB;QACtCC,gBAAgB,EAAE,WAAW;QAC7BC,cAAc,EAAE;MAClB,CAAC;IACH;EACF,CAAC;EACDC,OAAO,EAAE;IACPf,cAAcA,CAACgB,IAAI,EAAEC,IAAI,EAAEC,KAAK,EAAEZ,OAAO,EAAE;MACzC,IAAI,CAACH,WAAU,GAAIa,IAAI;MACvB,IAAI,CAACZ,QAAO,GAAIa,IAAI;MACpB,IAAI,CAACZ,WAAU,GAAIa,KAAK;MACxB,IAAI,CAACZ,OAAM,GAAIA,OAAM,IAAK,OAAO,EAAE;IACrC;EACF,CAAC;EACDa,OAAOA,CAAA,EAAG;IACR;IACA,IAAI,CAACnB,cAAc,CACjB,MAAM,EACN,YAAY,EACZ,oBAAoB,EACpB,gBACF,CAAC;EACH,CAAC;EACDoB,MAAM,EAAEzB,YAAY,CAAC;IACnB0B,OAAO,EAAEzB,gBAAgB,CAAC0B,OAAO,CAACC,GAAG,CAACC,QAAQ,CAAC;IAC/CC,MAAM,EAAE,CACN;MACEC,IAAI,EAAE,GAAG;MACTC,IAAI,EAAE,MAAM;MACZC,SAAS,EAAE9B;IACb;IACA;IAAA;EAEJ,CAAC;AACH,CAAC","ignoreList":[]},"metadata":{},"sourceType