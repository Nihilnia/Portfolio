{"ast":null,"code":"import { ref, computed, provide } from \"vue\";\nimport SideBar from \"@/components/layouts/SideBar.vue\";\nimport TopNavigator from \"@/components/UI/TopNavigator.vue\";\nimport BaseTitle from \"@/components/UI/BaseTitle.vue\";\nimport BaseSubtitle from \"@/components/UI/BaseSubtitle.vue\";\nexport default {\n  components: {\n    SideBar,\n    TopNavigator,\n    BaseTitle,\n    BaseSubtitle\n  },\n  props: [\"page-title\", \"top-title\", \"top-subtitle\", \"bgImage\"],\n  setup() {\n    const isSidebar = ref(true);\n    const setGridLayout = () => {\n      isSidebar.value = !isSidebar.value;\n    };\n    provide(\"isSidebar\", isSidebar);\n    provide(\"setGridLayout\", setGridLayout);\n    const compGridLayout = computed(() => isSidebar.value ? \"ten-nine\" : \"five-ninety-five\");\n    return {\n      isSidebar,\n      setGridLayout,\n      compGridLayout\n    };\n  },\n  computed: {\n    backgroundStyle() {\n      return {\n        backgroundImage: `url(${require(`@/assets/pics/${this.bgImage}`)})`\n      };\n    }\n  }\n};","map":{"version":3,"names":["ref","computed","provide","SideBar","TopNavigator","BaseTitle","BaseSubtitle","components","props","setup","isSidebar","setGridLayout","value","compGridLayout","backgroundStyle","backgroundImage","require","bgImage"],"sources":["C:\\Users\\Nihil\\Desktop\\re-licts\\src\\layouts\\MainLayout.vue"],"sourcesContent":["<template>\r\n  <section id=\"container-main\" :class=\"compGridLayout\">\r\n    <side-bar></side-bar>\r\n    <section id=\"side-right\">\r\n      <top-navigator :page-title=\"pageTitle\"></top-navigator>\r\n      <div id=\"container-top\">\r\n        <base-title :title=\"topTitle\"> </base-title>\r\n        <base-subtitle :title=\"topSubtitle\"> </base-subtitle>\r\n      </div>\r\n      <slot name=\"mainContent\"> </slot>\r\n    </section>\r\n  </section>\r\n</template>\r\n\r\n<script>\r\nimport { ref, computed, provide } from \"vue\";\r\nimport SideBar from \"@/components/layouts/SideBar.vue\";\r\nimport TopNavigator from \"@/components/UI/TopNavigator.vue\";\r\nimport BaseTitle from \"@/components/UI/BaseTitle.vue\";\r\nimport BaseSubtitle from \"@/components/UI/BaseSubtitle.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    SideBar,\r\n    TopNavigator,\r\n    BaseTitle,\r\n    BaseSubtitle,\r\n  },\r\n  props: [\"page-title\", \"top-title\", \"top-subtitle\", \"bgImage\"],\r\n  setup() {\r\n    const isSidebar = ref(true);\r\n\r\n    const setGridLayout = () => {\r\n      isSidebar.value = !isSidebar.value;\r\n    };\r\n\r\n    provide(\"isSidebar\", isSidebar);\r\n    provide(\"setGridLayout\", setGridLayout);\r\n\r\n    const compGridLayout = computed(() =>\r\n      isSidebar.value ? \"ten-nine\" : \"five-ninety-five\"\r\n    );\r\n\r\n    return {\r\n      isSidebar,\r\n      setGridLayout,\r\n      compGridLayout,\r\n    };\r\n  },\r\n  computed: {\r\n    backgroundStyle() {\r\n      return {\r\n        backgroundImage: `url(${require(`@/assets/pics/${this.bgImage}`)})`,\r\n      };\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n#container-main {\r\n  display: grid;\r\n  color: #fff;\r\n  background-size: contain;\r\n  background-position: center;\r\n  background-repeat: no-repeat;\r\n}\r\n\r\n.ten-nine {\r\n  grid-template-columns: 10% 90%;\r\n}\r\n\r\n.five-ninety-five {\r\n  grid-template-columns: 5% 95%;\r\n}\r\n\r\n#side-right {\r\n  padding: 0 1.5rem 1rem 1.5rem;\r\n}\r\n\r\n#side-right > header {\r\n  text-align: center;\r\n}\r\n\r\n#container-top {\r\n  margin-top: 44px;\r\n}\r\n\r\n@media all and (max-width: 1200px) {\r\n  #container-main {\r\n    grid-template-columns: 1fr;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAeA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,OAAM,QAAS,KAAK;AAC5C,OAAOC,OAAM,MAAO,kCAAkC;AACtD,OAAOC,YAAW,MAAO,kCAAkC;AAC3D,OAAOC,SAAQ,MAAO,+BAA+B;AACrD,OAAOC,YAAW,MAAO,kCAAkC;AAE3D,eAAe;EACbC,UAAU,EAAE;IACVJ,OAAO;IACPC,YAAY;IACZC,SAAS;IACTC;EACF,CAAC;EACDE,KAAK,EAAE,CAAC,YAAY,EAAE,WAAW,EAAE,cAAc,EAAE,SAAS